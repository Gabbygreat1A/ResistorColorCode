<SixColorResistor>:
	adaptive_size: True
	MDBoxLayout:
		size_hint: None, None
		pos_hint: {'center_y': .5}
		md_bg_color: 1, 1, 1, 1
		size: 20, 9
	MDBoxLayout:
		adaptive_size: True
		md_bg_color: .5, .3, .2, 1
